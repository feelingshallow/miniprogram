/*轮播图片*/
.scro{height:200px;display: flex;justify-content: center;align-items: center;z-index: 10;}
.scro .teachers_banner {margin-top: 20rpx;
  width: 100%;
  /* background: url(../images/teachers_banner.jpg) no-repeat center top; */
  background-size: cover;
  position: relative;
  overflow: hidden;
  margin-bottom: 20rpx;
}

.scro  .teachers_b {
  position: relative;
}

.scro   .slides {
  margin: 0 auto;
  width: 100%;
  height: 185px;
  position: relative;
  display: flex;
  flex-direction: column;
  z-index: 5;
}

.scro .ul {
  display: flex;
  width: 100%;
  /* margin-top: -270rpx; */
  height: 185px;
  position: relative;
}

.ul .li {
  position: absolute;
  top:0;
  width: 90%;
  margin-left: 5%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  align-items: flex-start;
  -webkit-box-align: flex-start;
  -webkit-align-items: flex-start;
  background: #fff;
  overflow: hidden;
  /* width:343px; */
  height: 83px;
  /* background:linear-gradient(270deg,rgba(113,157,255,1) 0%,rgba(135,205,255,1) 100%); */
  box-shadow: 0px 9px 17px -9px rgba(0, 0, 0, 0.5);
  border-radius: 8px;
}

.ul .li .img {
  width: 100%;
  height: 100%;
}

.ul .li .kc{height: 100%;z-index: 999;position: absolute;top:0;
  width: 100%;display: flex;justify-content: space-between;align-items: center;flex-direction: row;
}
.kc .left{display: flex;width: 70%;margin-left: 20rpx;
flex-direction: column;justify-content: flex-start;}

.kc .right{display: flex;justify-content: center;align-items: center;
  width:30%;
height:72px;
font-size:50px;
font-family:Helvetica-Bold,Helvetica;
font-weight:bold;
color:rgba(255,255,255,1);
line-height:72px;
letter-spacing:4px;
}

.left .top{
  margin-bottom: 10rpx;
height:30px;
font-size:22px;
font-family:PingFangSC-Semibold,PingFang SC;
font-weight:600;
color:rgba(255,255,255,1);
line-height:30px;
overflow: hidden;display: -webkit-box; -webkit-line-clamp: 1;-webkit-box-orient: vertical;text-overflow:ellipsis;
}

.left .bottom{
  display: flex;justify-content: flex-start;align-items: center;width: 100%;
}

.bottom .vi{
  white-space: nowrap;
height:17px;
font-size:12px;
font-family:PingFangSC-Semibold,PingFang SC;
font-weight:600;
color:rgba(255,255,255,1);
line-height:17px;
overflow: hidden;display: -webkit-box; -webkit-line-clamp: 1;-webkit-box-orient: vertical;text-overflow:ellipsis;
}
.bottom .imgs{width:19px;height:20px;}

.slide_right {
  /* width:440px;*/
  padding: 40px;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  min-width: 0;
}

.slide_right h3 {
  font: 400 30px/18px "Microsoft Yahei";
  color: #222;
}

.slide_right h3 span {
  display: inline-block;
  margin-left: 10px;
  font: 400 14px/36px "Microsoft Yahei";
  color: #555;
}

.slide_right p {
  padding: 20px 0 30px;
  color: #555;
  font: 400 14px/24px "Microsoft Yahei";
  border-bottom: 1px solid #dbdbdb;
}

.slide_right dl {
  padding-top: 30px;
}

.slide_right dd {
  float: left;
  width: 33.3%;
  color: #777;
  font: 400 12px/24px "Microsoft Yahei";
}

.slide_right dd h3 {
  color: #ff9000;
  margin-bottom: 20px;
}

.arrow {
  /* display: none;*/
}

.arrow .prev, .arrow .next {
  position: absolute;
  width: 64px;
  top: 38%;
  z-index: 9;
  font: 700 96px 'simsun';
  opacity: 0.3;
  color: #fff;
  cursor: pointer;
}

.arrow .prev {
  /* left: -220px; */
}

.arrow .next {
  /* right: -220px; */
}

.arrow .prev:hover, .arrow .next:hover {
  color: #00a0e9;
  opacity: 0.7;
}

