page {
  background-color: white;
}

.title {
  width: 90%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 5%;
}

.title .left {
  display: flex;
  align-items: center;
}

.title .left .logo {
  width: 122rpx;
  height: 122rpx;
  margin-right: 20rpx;
}

.title .left .name {
  width: 224rpx;
  height: 80rpx;
  font-size: 56rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(55, 52, 71, 1);
  line-height: 80rpx;
}

.title .avatar {
  width: 72rpx;
  height: 72rpx;
  background: rgba(254, 241, 0, 1);
  box-shadow: 0px 8px 16px -8px rgba(155, 116, 0, 1);
  border-radius: 50%;
}

.slider {
  width: 100%;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}

.slider .le-img {
  /* width: 50rpx; */
  height: 192rpx;
  display: block;
  transform: scale(0.9);
  transition: all 0.3s ease;
  background: rgba(185, 185, 185, 1);
  box-shadow: 0px 12px 25px -7px rgba(198, 60, 11, 0.3);
  border-radius: 8px;
}

.slider .le-img.le-active {
  transform: scale(1);
  width: 100%;
  height: 232rpx;
}



.znkb {
  width: 90%;
  margin-left: 5%;
  display: flex;
  justify-content: space-between;
}

.znkb .name {
