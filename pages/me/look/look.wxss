page {
  background-color: rgba(252, 252, 252, 1);
}

.tui-header-box {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9998;
  background-color: rgba(252, 252, 252, 1);
}

.tui-header {
  width: 100%;
  font-size: 30rpx;
  line-height: 18px;
  font-weight: bold;
  height: 32px;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  color: white;
  position: relative;
}

.tui-header .bb {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
text-align: center;
}

.tui-header .bb .name {
 width: 100%;
font-size:24px;
font-family:PingFangSC-Semibold,PingFang SC;
font-weight:600;
color:rgba(55,52,71,1);
letter-spacing:1px;
white-space: nowrap;
}

.tui-header .back {
  position: absolute;
  width: 14px;
  height: 23px;
  margin-left:35rpx;
}
.con{overflow-y:auto;margin-top:20rpx;margin-bottom: 75px;}
.con .content {width: 100%;
  display: flex;justify-content: center;align-items: center;
}
.content image{width: 343px;height: 485px;}


.con .memberName{width: 100%;display: flex;justify-content: center;align-items: center;margin-bottom: 40rpx;
height:25px;
font-size:18px;
font-family:PingFangSC-Semibold,PingFang SC;
font-weight:600;
color:rgba(55,52,71,1);
line-height:25px;}

.con .yaoqiu{width: 100%;display: flex;justify-content: center;align-items: center;
height:20px;
font-size:14px;
