font-weight:600;
color:rgba(55,52,71,1);
line-height:20px;}

.con .yq{width: 75%;margin-left: 12.5%;display: flex;flex-direction: column;justify-content: center;margin-top: 16rpx ;align-items: center;margin-bottom: 40rpx ;
height:280px;
font-size:11px;
font-family:PingFangSC-Semibold,PingFang SC;
font-weight:600;
color:rgba(55,52,71,1);
line-height:20px;
letter-spacing:1px;}

.btn{position: fixed;bottom: 0;display: flex;justify-content: center;align-items: center;
  width:100%;
height:72px;
background:linear-gradient(180deg,rgba(155,239,150,1) 0%,rgba(90,173,49,1) 100%);
box-shadow:0px -4px 16px -1px rgba(100,100,100,0.2);
border-radius:20px 20px 0px 0px;
font-size:22px;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(255,255,255,1);
line-height:30px;
letter-spacing:8px;
}

.btn text{font-size:16px;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(255,255,255,1);
line-height:22px;
letter-spacing:2px;}


